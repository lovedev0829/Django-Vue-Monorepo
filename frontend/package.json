        "eslint": "^8.30.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-nuxt": "^4.0.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-vue": "^9.4.0",
        "nuxt-primevue": "^0.3.1",
        "prettier": "^3.2.5",
        "sass": "^1.72.0",
        "sass-loader": "^14.1.1",
        "tailwindcss": "^3.4.1",
        "yarn": "^1.22.22"
    }
