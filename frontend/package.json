        "@vee-validate/yup": "^4.13.2",
        "axios": "^1.6.8",
        "eslint": "8.30.0",
        "jwt-decode": "^4.0.0",
        "next-auth": "^4.21.1",
        "nuxt": "^3.11.0",
        "pinia": "^2.1.7",
        "postcss": "^8.4.36",
        "primeicons": "^6.0.1",
        "primevue": "^3.53.0",
        "vee-validate": "^4.13.2",
        "vue": "^3.4.21",
        "vue-router": "^4.3.0",
        "yup": "^1.4.0"
    },
    "devDependencies": {
        "@babel/eslint-parser": "^7.23.10",
